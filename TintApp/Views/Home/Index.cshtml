            $(document).ready(function () {
            $('#ServiceCategoryId').change(function () {
                var categoryId = $(this).val();
                if (categoryId) {
                    $.ajax({
                        url: '/Booking/GetItemsByCategory',
                        type: 'GET',
                        data: { categoryId: categoryId },
                        success: function (data) {
                            let $itemSelect = $('#ServiceItemId');
                            $itemSelect.empty().append('<option value="">Select Item</option>');
                            $.each(data, function (i, item) {
                                $itemSelect.append('<option value="' + item.id + '">' + item.name + '</option>');
                            });
                            $itemSelect.removeClass('d-none');
                        }
                    });
                }
            });
        });

        const allItems = @Html.Raw(Json.Serialize(ViewBag.ServiceItems));

        function loadItemsByCategory(categoryId) {
            const itemDropdown = document.getElementById('ItemId');
            itemDropdown.innerHTML = '<option selected disabled>-- Select Item --</option>';

            const matchedItems = allItems.filter(item => item.serviceCategoryId === parseInt(categoryId));

            matchedItems.forEach(item => {
                const option = document.createElement('option');
                option.value = item.id;
                option.textContent = item.name;
                itemDropdown.appendChild(option);
            });

            itemDropdown.classList.remove('d-none');
        }

    </script>
}

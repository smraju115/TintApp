<!-- Services-->
<section class="page-section" id="services">
    <div class="container">
        <div class="text-center">
            <h2 class="section-heading text-uppercase">Services</h2>
            <h3 class="section-subheading text-muted">Lorem ipsum dolor sit amet consectetur.</h3>
        </div>
        <div class="row g-5">
            @foreach (var offer in Model)
            {
                <div class="col-md-4 mb-5">
                    <div class="offer-card">
                        <img src="@offer.ImageUrl" alt="Offer Image" class="offer-image">
                        <div class="offer-content">
                            <h5 class="fw-bold">@offer.CategoryName</h5>
                            <p class="text-muted mb-1">@offer.Title</p>
                            <a asp-controller="ServiceCategory" asp-action="PublicDetails" asp-route-categoryId="@offer.Id" class="offer-button">View Details</a>
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>

